        [       :LNOT: :DEF: Name_Clash
        GBLL            Name_Clash
Name_Clash      SETL   {TRUE}                           ; Enable checking for SWI name clashes
        ]
        [       :LNOT: :DEF: Name_Generator
        GBLS            Name_Generator
Name_Generator  SETS    "Acorn"                         ; Generate the set of SWIs used here at Acorn
        ]

;   The Chunk macro takes its first argument, on the line before the MACRO
;   invocation, as part of a symbol to generate (this symbol is made by
;   appending the string "SWI_Base" to the argument.  This value is the base
;   value allocated to the SWI chunk.  This value increments by &40
;   each time and should be used by module source code to assemble the module
;   header with e.g. DCD MyChunkSWI_Base
;
;   The argument after the invocation (second) is the estimated value that the
;   SWI chunk will have, this value is ASSERTed by the MACRO and an assembly
;   will fail if the estimate is wrong.
;
;   The next (third) argument is the name of the vendor or company.  This is
;   used to group SWI chunk symbols together for a single vendor.  The next
;   (fourth) argument is optional and if not present defaults to the
;   value of the first argument, it is used to check that the SWI name prefix
;   allocated is unique.  When assembled with the variable SWI_Name_Clash true
;   extra code is assembled to ensure that every name is indeed unique, an EQU
;   will fail with a "Multiply or incompatibly defined symbol" error if a
;   non-unique name is found.  To prevent this from occuring when it is
;   specifically desired to have the same name prefix for more than one chunk,
;   a fifth optional argument "NameOK" can be added to suppress this check.
;   The values of the names are generated with variable names with "SWI_Name"
;   appended.  See Hdr:SWIs for example output.
;
;   The ChunkM macro should be used to allocate a chunk for wimp messages. 
;   It takes the same first three arguments as the Chunk macro, it doesn't
;   need a forth string argument or a NameOK argument.  The ChunkM macro
;   generates symbols of the form ThingMessage with the chunk number and
;   ThingMessage_Base with the actual value i.e. &0008xxxx.  Note that
;   Chunks allocated for messages should be in the application area.  If an
;   allocation is going to be used both for SWIs and for messages then use
;   the Chunk macro and note the message use in the comment field.


; ***********************************************
; ***  InitialiseFile - Helps with the        ***
; ***  generation of Hdr:SWIs                 ***  
; ***  cf BlankLineInFile, and TerminateFile  ***
; ***********************************************
        MACRO
        InitialiseFile
        [               "Name_Generator" <> ""
        =               "        SUBT    Specific SWI definitions ==> Hdr:SWIs", 10
        =               "", 10
        =               "OldOpt  SETA    {OPT}", 10
        =               "        OPT     OptNoList+OptNoP1List", 10
        =               "", 10
        =               "        ; Generated from Hdr:ISV-SWIs", 10
        =               "        ; For the vendor """, "$Name_Generator", """", 10
        =               "", 10
        ]  
        MEND

; ********************************************************
; ***  Chunk - Allocates and checks SWI chunk numbers  ***
; ********************************************************
        MACRO
$name   Chunk           $value,$company,$string,$nameok
        LCLS            spaces
spaces  SETS            "                                                     "
        LCLS            swi
swi     SETS            "$company" :CC: "_" :CC: "$name" :CC: "SWI"
$swi    #               1                               ; Generate the chunk number
        ASSERT          (&$value :AND: &1FFFF) = (&40 * $swi)
        [               Name_Clash
        [               "$string" = ""
swi     SETS            "SWI_String_" :CC: "$name"
        |
swi     SETS            "SWI_String_" :CC: "$string"
        ]
        [               :DEF: $swi                      ; True if name clashes
        [               "$nameok" = ""
        !               0, "Name clash with " :CC: $swi :CC: " found!"
        ]
        |               ; Name clash found
        GBLS            $swi                            ; Use symbol table to check for unique name
$swi    SETS            "$company" :CC: "_" :CC: "$name"
        [               "$nameok" = "NameOK"
        !               0, "Name clash expected but not found!"
        ]
        ]               ; Name clash found
        ]               ; Name clash checking enabled
        [               (Name_Generator = "$company"):LOR:(Name_Generator = "*")
swi     SETS            "$name" :CC: "SWI" :CC: "_Name"
        =               "                                GBLS    "
        =               "$swi"
        =               10
swi     SETS            "$company" :CC: "_" :CC: "$name" :CC: "SWI"
        [               "$company" = "Acorn"
        =               ("$name" :CC: "SWI" :CC: "$spaces") :LEFT: 31
        =               " EQU     &"
        =               :STR: $swi
        =               10
        ]
        =               ("$name" :CC: "SWI_Base" :CC: "$spaces") :LEFT: 31
        =               " EQU     &"
swi     SETS            "&" :CC: "$value"
        =               :STR: $swi
        =               10
swi     SETS            (("$name" :CC: "SWI" :CC: "_Name" :CC: "$spaces") :LEFT: 31) :CC: " SETS    """""""""""""""""
        [               "$string" = ""
swi     SETS            "$swi" :CC: "$name" :CC: """"""""""
        |
swi     SETS            "$swi" :CC: "$string" :CC: """"""""""
        ]
        [               "$nameok" = "NameOK"
swi     SETS            (("$swi" :CC: "$spaces") :LEFT: 64) :CC: "; Possible name clash"
        |
swi     SETS            "$swi"
        ]
        =               "$swi"
        =               10, 10
        ]               ; Making a file
        MEND

; *************************************************************
; ***  ChunkM - Allocates and checks Message chunk numbers  ***
; *************************************************************
        MACRO
$name   ChunkM          $value,$company
        LCLS            spaces
spaces  SETS            "                                                     "
        LCLS            swi
swi     SETS            "$company" :CC: "_" :CC: "$name" :CC: "Messages"
$swi    #               1
        ASSERT          (&$value :AND: &1FFFF) = (&40 * $swi)
        [               (Name_Generator = "$company") :LOR: (Name_Generator = "*")
swi     SETS            "$company" :CC: "_" :CC: "$name" :CC: "Messages"
        [               "$company" = "Acorn"
        =               ("$name" :CC: "Message" :CC: "" :CC: "$spaces") :LEFT: 31
        =               " EQU     &"
        =               :STR: $swi
        =               10
        ]
        =               ("$name" :CC: "Message" :CC: "_Base" :CC: "$spaces") :LEFT: 31
        =               " EQU     &"
swi     SETS            "&" :CC: "$value"
        =               :STR: $swi
        =               10, 10
        ]               ; Making a file
        MEND

; ****************************************************
; ***  LineInFile - Helps with the                 ***
; ***  generation of Hdr:SWIs and Hdr:Types        ***  
; ***  cf InitialiseSWIFile, and TerminateSWIFile  ***
; ****************************************************
        MACRO
        LineInFile      $comment
        [               "Name_Generator" <> ""
        [               "$comment" <> ""
        =               10
        =               "        ; ", "$comment"
        =               10
        ]
        =               10
        ]
        MEND

; ******************************************************
; ***  TerminateFile - Helps with the                ***
; ***  generation of Hdr:SWIs and Hdr:Types          ***  
; ***  cf BlankLineInSWIFile, and InitialiseSWIFile  ***
; ******************************************************
        MACRO
        TerminateFile
        [               "Name_Generator" <> ""
        =               "        OPT     OldOpt", 10
        =               10
        =               "        END", 10
        ]
        MEND

; *************************************************
; *************************************************
; ***                                           ***
; ***  Actual SWI Chunk definitions start here  ***
; ***                                           ***
; *************************************************
; *************************************************

                        InitialiseFile

                        ;  The following line is a comment intended for readers of Hdr:SWIs
                        LineInFile      "***  Don't edit this file, use Hdr:ISV-SWIs instead!  ***"

                        LineInFile      "Operating System SWI Chunks"


