;
;    &000 - &0FF  For user's personal things, no allocations needed here.
;    &100 - &3FF  For user's distributed applications e.g. Public Domain
;                 software), allocated by Acorn (Customer Services).
;    &400 - &DFF  For commercial third party use, allocated by Acorn (ISV support).
;    &E00 - &FFF  For use by Acorn for internal and external applications
;                 also for more "public" types e.g. TIFF.
;

        [       :LNOT: :DEF: Name_Clash
        GBLL            Name_Clash
Name_Clash SETL     {TRUE}                              ; Enable checking for File-type name clashes
        ]
        [       :LNOT: :DEF: Name_Generator
        GBLS            Name_Generator
Name_Generator SETS "Acorn"                             ; Generate the set of File-types used here at Acorn
        ]

;   There are only 4096 distinct file types available, they are numbered
; from &000 to &FFF.  Each file type has an associated name of up to 8
; characters in length.  The name is displayed in directory viewers and
; file information boxes.  It is also an acceptable argument to *SetType.
;
;   The numbering space is divided up as follows;
;
;    &000 - &0FF  For user's personal things, no allocations needed here.
;    &100 - &3FF  For user's distributed applications e.g. Public Domain
;                 software), allocated by Acorn (Customer Services).
;    &400 - &DFF  For commercial third party use, allocated by Acorn (ISV support).
;    &E00 - &FFF  For use by Acorn for internal and external applications
;                 also for more "public" types e.g. TIFF.
;
;
; ***********************************************
; ***  InitialiseFile - Helps with the        ***
; ***  generation of Hdr:Types                ***  
; ***  cf BlankLineInFile, and TerminateFile  ***
; ***********************************************
        MACRO
        InitialiseFile
        [               "Name_Generator" <> ""
        =               "        SUBT    Specific File Type definitions ==> Hdr:FileTypes", 10
        =               "", 10
        =               "OldOpt  SETA    {OPT}", 10
        =               "        OPT     OptNoList+OptNoP1List", 10
        =               "", 10
        =               "        ; Generated from Hdr:ISV-Types", 10
        =               "        ; For the vendor """, "$Name_Generator", """", 10
        =               "", 10
        ]  
        MEND

; ****************************************************
; ***  FileType - Allocates and checks File Types  ***
; ****************************************************
        MACRO                                           ; See Hdr:ISV-Types for use
$name   FileType        $value,$company,$string
        #               -1
        [               "$string" = ""
$name   FileTypeD       $value, $company
        |
$name   FileTypeD       $value, $company, "$string"
        ]
        MEND

; ***************************************************************
; ***  FileTypeD - Allocates and checks DUPLICATE File Types  ***
; ***************************************************************
        GBLA            UsedFileTypes
UsedFileTypes           SETA    0
        MACRO                                           ; See Hdr:ISV-Types for use
$name   FileTypeD       $value,$company,$string
UsedFileTypes           SETA    UsedFileTypes + 1
        LCLS            spaces
spaces  SETS            "                                                     "
        LCLS            type
        LCLS            actual
type    SETS            "$company" :CC: "_" :CC: "$name"
$type   #               1                               ; Generate the actual file type
        #               -1
        ASSERT          (&$value ) = ($type)
        [               "$string" = ""
        [               :LEN: "$name" <= 8
actual  SETS            "$name"
        |
actual  SETS            "FT_" :CC: "$value"
        ]
        |
        ASSERT          :LEN: "$string" <= 8
actual  SETS            "$string"
        ]
        [               Name_Clash
type    SETS            "|FileType_String_" :CC: actual :CC: "|"
        [               :DEF: $type                     ; True if name clashes
        LCLS            other
other   SETS            $type
        !               0, "*** " :CC: "$company" :CC: "_" :CC: "$name" :CC: " &" :CC: "$value" :CC: " """ :CC: actual :CC: """ CLASHES WITH " :CC: $type :CC: " &" :CC: ((:STR: $other) :RIGHT: 3) :CC: " ***"
        ]               ; Name clash found
        GBLS            $type
$type   SETS            "$company" :CC: "_" :CC: "$name"
        ]               ; Name clash checking enabled
        [               (Name_Generator = "$company"):LOR:(Name_Generator = "*")
type    SETS            "FileType_" :CC: "$name" :CC: "_Name"
        =               "                                GBLS    "
        =               "$type"
        =               10
        [               ((("FileType_" :CC: "$name" :CC: "$spaces") :LEFT: 32) :RIGHT: 1) = " "
        =               ("FileType_" :CC: "$name" :CC: "$spaces") :LEFT: 32
        |
        =               "FileType_" :CC: "$name" :CC: " "
        ]
        =               "EQU     &"
type    SETS            "$company" :CC: "_" :CC: "$name"
        =               :STR: $type
        =               10
        [               ((("FileType_" :CC: "$name" :CC: "_Name" :CC: "$spaces") :LEFT: 32) :RIGHT: 1) = " "
type    SETS            (("FileType_" :CC: "$name" :CC: "_Name" :CC: "$spaces") :LEFT: 32)
        |
type    SETS            "FileType_" :CC: "$name" :CC: "_Name" :CC: " "
        ]
type    SETS            "$type" :CC: "SETS    """"""""" :CC: actual :CC: """"""""""
type    SETS            "$type"
        =               "$type"
        =               10, 10
        ]               ; Making a file
        MEND

; ****************************************************
; ***  LineInFile - Helps with the                 ***
; ***  generation of Hdr:SWIs and Hdr:Types        ***  
; ***  cf InitialiseSWIFile, and TerminateSWIFile  ***
; ****************************************************
        MACRO
        LineInFile      $comment
        [               "Name_Generator" <> ""
        [               "$comment" <> ""
        =               10
        =               "        ; ", "$comment"
        =               10
        ]
        =               10
        ]
        MEND

; ***************************************************************
; ***  VoidTypesUntil - Skip over some file type allocations  ***
; ***************************************************************
        GBLA    UnusedFileTypes
UnusedFileTypes SETA 0
        MACRO
$label  VoidTypesUntil    $newbase
        [       "$label" = ""
        LCLA    value
value   SETA    &$newbase
        ASSERT  @ > value
        [       :LNOT: (@ > value)
        !       0, "@ = " :CC: (:STR: @) :CC: " - NewBase = " :CC: (:STR: value)
        ]
        [       @ = value
        !       1, "VoidTypesUntil is redundent"
        |
UnusedFileTypes SETA UnusedFileTypes + (@ - value)
        ^       value
        ]
        |
        !       0, "No lable on VoidTypesUntil MACRO invocations please!"
        ]
        MEND

; ******************************************************
; ***  TerminateFile - Helps with the                ***
; ***  generation of Hdr:SWIs and Hdr:Types          ***  
; ***  cf BlankLineInSWIFile, and InitialiseSWIFile  ***
; ******************************************************
        MACRO
        TerminateFile
        [               "Name_Generator" <> ""
        =               "        OPT     OldOpt", 10
        =               "", 10
        =               "        END", 10
        ]
        MEND


