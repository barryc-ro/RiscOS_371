struct proc
{
    uid_t    p_uid;		/* user id, used to direct tty signals */
    short    p_pgrp;		/* name of process group leader */
    char    *p_wchan;		/* event process is awaiting */
};

extern struct proc *pfind(short pgrp);

/* EOF proc.h */
