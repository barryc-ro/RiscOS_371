struct rusage {
        int     ru_msgsnd;
        int     ru_msgrcv;
};
 
struct  user {
        struct  proc *u_procp;
        int     u_error;
        int     u_uid;
        struct  rusage u_ru; 
};

extern  struct user u;
extern int selwait;


#include "sys/errno.h"

/* EOF user.h */
