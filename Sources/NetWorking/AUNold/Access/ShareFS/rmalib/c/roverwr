#include <string.h>

#include "kernel.h"
#include "swis.h"

#include "ralloc.h"

void roverwritten(void *err)
{
    _kernel_oserror s = *(_kernel_oserror *) err;
    strcat(s.errmess, " Continue?");
    if (_swi(Wimp_ReportError, _INR(0, 2) | _RETURN(1),
	     err, 3, "Debugging Check") != 1)
	_swi(OS_GenerateError, _IN(0), err);
}

/* EOF roverwr.c */
