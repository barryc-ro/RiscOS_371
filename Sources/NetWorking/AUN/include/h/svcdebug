#ifdef DEBUG
extern int debug;
#else
# define debug 0
#endif

extern void Printf(char *format, ...);

#define PRINTF	if (debug) Printf
#define N(x)	(x ? x : "NULL")

/*
 * trigger level for "super debugs"
 */
extern int superdebug;
#define SDBTRIGGER 16

