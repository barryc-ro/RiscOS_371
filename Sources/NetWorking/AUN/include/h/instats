/*
 * Econet statistics
 */
typedef struct eco_stats {
    int	     eco_rxcnt;
    int	     eco_txcnt;
    int	     eco_linejammed;
    int	     eco_neterror;
    int	     eco_notlistening;
    int	     eco_noclock;
    int	     eco_other;
    int	     eco_txhwat;
    int	     eco_txqlen;
    int	     eco_txrej;
} Ecostats;

#include "netinet/in.h"
#include "netinet/ip.h"
#include "netinet/ip_var.h"
#include "netinet/tcp_var.h"
#include "netinet/udp_var.h"

