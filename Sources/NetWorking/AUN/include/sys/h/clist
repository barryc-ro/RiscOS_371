/*
 * Raw structures for the character list routines.
 */
struct cblock {
	struct cblock *c_next;
	char	c_info[CBSIZE];
};
#ifdef	KERNEL
#ifdef	DYNALLOC
struct	cblock *cfree;
#else	DYNALLOC
struct	cblock stat_cfree[NCLIST], *cfree;
#endif	DYNALLOC
int	nclist;
struct	cblock *cfreelist;
int	cfreecount;
#endif

/* EOF clist.h */
