#include "util.h"
#include "swis.h"

#define VduVariable_WindowWidth         256

int readwidth(void)
{
    int iblk[2];
    int width;
    iblk[0] = VduVariable_WindowWidth;
    iblk[1] = -1;
    _swix(OS_ReadVduVariables, _INR(0, 1), &iblk, &width);
    return width;
}

/* EOF readwidth.c */
