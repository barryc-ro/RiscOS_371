#include <string.h>

#include "util.h"

/*
 * Insert string s at the beginning of string d
 */
void strins(char *d, const char *s)
{
    int l = strlen(s);
    int m = strlen(d);
    memmove(&d[l], d, m + 1);
    memmove(d, s, l);
}

/* EOF strins.c */
