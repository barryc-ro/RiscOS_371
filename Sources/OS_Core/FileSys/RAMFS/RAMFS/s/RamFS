;>RamFS

; This GETs all relevant files for RAMFS
; This expects to be AASMed in the parent of directory asm
; which is expected to contain all the sources.

        GBLL    test_version
test_version SETL {FALSE}
        GET     Version
        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Services
        GET     Hdr:ModHand
;        GET     Hdr:NewSpace
        GET     Hdr:UpCall
        GET     Hdr:Tokens
        GET     Hdr:Proc
        GET     Hdr:MsgTrans
        GET     Hdr:FileCore
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors

        GET     s.MyMacros
        GET     s.Defns
        GET     s.DebugOpts
        GET     s.Header
        GET     s.Errors
        GET     s.RamFS00
        GET     s.RamFS05
        GET     s.RamFS15
        GET     s.TokenHelp
        GET     s.RamFS50
        GET     s.MsgCode
        END
