VPATH = @ <Support$Dir>

OBJS = o.main

SelectTable: o.main
   Link -output SelectTable ${OBJS} OSLib:o.OSLib CLib:o.Stubs

.SUFFIXES: .c .o .def .h
.c.o:;     cc -c -throwback -IOS:,Support: -wp $<
.def.h:;   DefMod -h < $< > $@
.def.o:;   DefMod -l -o l.$* < $<
           LibFile -o -c $@ -via ViaFile
           Wipe l.$* r~c
