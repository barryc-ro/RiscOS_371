internal
        Branch  rmensure
        Branch  string_length
        Branch  string_copy
        Branch  string_to_buffer
        Branch  string_copy_chk
        Branch  copy_template
        Branch  __zap_gadget
        Branch  __read_gadget
        Branch  __zap_window
        Branch  create_from_template
        Branch  delete_gadget
        Branch  __read_window
        DCD     0
        Branch  mem_allocate
        Branch  mem_free
        Branch  mem_free_all
        B       prv_calloc
        B       prv_realloc
        B       prv_free
        B       prv_kernel_swi

jumptable     
        STMFD   sp!,{lr} 
01
        LDR     lr,[r0],#4
        CMP     lr,#-2
        [ IncludeCLib
        ADREQL  r1,KernelBranches
        |
        ADREQ   r1,where
        LDREQ   r1,[r1]
        ]
        BEQ     %FT05

        CMP     lr,#-4
        [ IncludeCLib
        ADREQ   r1,CLibBranches
        |
        ADREQ   r1,where
        LDREQ   r1,[r1]
        ADDEQ   r1,r1,#48*4
        ]
        BEQ     %FT05

        CMP     lr,#-3
        ADREQ   r1,internal
        BEQ     %FT05

        CMP     lr,#-1
        MOVEQ   r0,#0
        LDMEQFD sp!,{pc}^

        STMFD   sp!,{r0}
        MOV     r0,lr
        LDR     r1,=&82c41
        SWI     &20030
        TEQ     r1,#0
        ADRNE   r0,error
        STRNE   r0,[sp]
        MOVEQ   r1,r0
        LDMFD   sp!,{r0}
        LDMNEFD sp!,{pc}^
05
; r1 -> where to find them
        BL      setupjump   
        B       %BT01

error
        DCD     &82c40
        DCB     "Unable to dynamically link library section",0
        ALIGN

setupjump
; r1 -> full jump table
; r0 -> jump table to fill in
loop
        LDR     r2,[r0]
        TEQ     r2,#0
  [ {TRUE} ;StrongARM
        BMI     SA_synch_then_exit
  |
        MOVMIS  pc,lr
  ]
        LDR     r3,[r1,r2, LSL #2]
        ADD     r3,r3,r1, LSR #2
        ADD     r3,r3,r2
        SUB     r3,r3,r0, LSR #2        
        BIC     r3,r3,#&ff000000
        ORR     r3,r3,#&ea000000                ; B xxx
        STR     r3,[r0],#4
        B       loop

  [ {TRUE}  ;StrongARM
SA_synch_then_exit
    STMDB  sp!,{r0,lr}
    MOV    r0,#0                    ;means full synchronise
    SWI    XOS_SynchroniseCodeAreas ;do the necessary
    LDMIA  sp!,{r0,lr}   
    MOVS   pc,lr
  ]
         
        [ :LNOT: IncludeCLib
        IMPORT |_kernel_init|

where
        &      |_kernel_init|
        ]
                           
        IMPORT  |TS_SWI_handler|
Generic_SWI_handler
        STMFD   sp!,{v1,lr}
        MOV     a4,sp
        BL      TS_SWI_handler
        LDMFD   sp!,{v1,pc}^

        END
