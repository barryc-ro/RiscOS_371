
        AREA |JumpCompute|, CODE, READONLY

                GBLL    IncludeCLib
IncludeCLib     SETL    {TRUE}

        EXPORT |jumptable|
        EXPORT Generic_SWI_handler

        MACRO
        Branch  $func
        IMPORT  $func
        B       $func
        MEND

X * &20000

XOS_SynchroniseCodeAreas * &2006e
XOS_CallASWI             * &2006f

; from _kernel_swi - doesn't copy errors
|prv_kernel_swi|
  [ {TRUE} ;StrongARM
        MOV     ip, sp
        STMFD   sp!, {a3, v1-v6, sl, fp, ip, r14, pc}
        SUB     fp, ip, #4
        BIC     r10, a1, #&80000000
        TST     a1, #&80000000          ; non-X bit requested?
        ORREQ   r10, r10, #X
        LDMIA   r1, {r0-r9}
        SWI     XOS_CallASWI
        LDMFD   sp!, {ip}
        STMIA   ip, {r0 - r9}
        MOVVC   a1, #0
        LDMDB   fp, {v1-v6, sl, fp, sp, pc}^
  |
        ; Set up a proper frame here, so if an error happens (and not X)
        ; a sensible traceback can be given.
        MOV     ip, sp
        STMFD   sp!, {a3, v1-v6, fp, ip, r14, pc}
        SUB     fp, ip, #4
        ; be kind to fault handler if there is an error.
        ADR     a4, AfterSWI-4
        SUB     a4, a4, sp
        MOV     a4, a4, LSR #2
        BIC     a4, a4, #&ff000000
        ADD     a4, a4, #&ea000000      ; B always
        TST     a1, #&80000000          ; non-X bit requested?
        ORR     a1, a1, #&EF000000      ; SWI + Always
        ORREQ   a1, a1, #X
        STMFD   sp!, {a1, a4}
        LDMIA   a2, {r0 - r9}
        MOV     pc, sp
AfterSWI
        ADD     sp, sp, #8
        LDMFD   sp!, {ip}
        STMIA   ip, {r0 - r9}
        MOVVC   a1, #0
        LDMDB   fp, {v1-v6, fp, sp, pc}^
  ]

XOS_Module * &2001e

prv_calloc
        STMFD   sp!,{lr}
        MOV     r3,r0
prv_calloc2
        MOV     r0,#6                   ; Claim
        SWI     XOS_Module
        MOVVS   r0,#0
        LDMVSFD sp!,{pc}^               ; void *
; calloc so need to clear memory
        MOV     r0,r2
        MOV     r1,#0
        TST     r3,#3
        BNE     bytes_clear
05
        STR     r1,[r2],#4
        SUBS    r3,r3,#4
        STRNE   r1,[r2],#4
        SUBNES  r3,r3,#4                ; bit of speed up
        BNE     %BT05
        LDMFD   sp!,{pc}^

bytes_clear
10
        CMP     r3,#3
        STRGT   r1,[r2],#4
        SUBGT   r3,r3,#4
        BGT     %BT10
; r3 >= 1 as r3 was an incomplete number of words
15
        STRB    r1,[r2],#1
        SUBS    r3,r3,#1
        BNE     %BT15
        LDMFD   sp!,{pc}^
        
prv_realloc
        STMFD   sp!,{lr}
        MOVS    r2,r0
        MOV     r3,r1
        BEQ     prv_calloc2

        MOV     r0,#13
        SWI     XOS_Module
        MOVVS   r0,#0                   ; void *
        MOVVC   r0,r2
        LDMFD   sp!,{pc}^

prv_free
        MOV     r3,lr
        MOV     r2,r0
        MOV     r0,#7                   ; Free
        SWI     XOS_Module
        MOVS    pc,r3                   ; void

        [ IncludeCLib
