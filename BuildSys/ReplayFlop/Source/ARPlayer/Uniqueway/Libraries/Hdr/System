r11_fiq RN      11
r12_fiq RN      12
r13_fiq RN      13
r14_fiq RN      14

r13_irq RN      13
r14_irq RN      14

r13_svc RN      13
r14_svc RN      14

wp      RN      r12

sp      RN      r13
; No defined sp_usr or sp_fiq - application and FIQ owner dependent
sp_irq  RN      r13_irq
sp_svc  RN      r13_svc
stack   RN      r13

lr      RN      r14
lr_usr  RN      r14_usr
lr_fiq  RN      r14_fiq
lr_irq  RN      r14_irq
lr_svc  RN      r14_svc
